{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist/",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "paths": {
    "core-js/es7/reflect": [
      "../node_modules/core-js/proposals/reflect-metadata"
    ],
    "core-js/es6/": [
      "./node_modules/core-js/es/"
    ]
  },
  "exclude": [
    "node_modules/*"
  ]
}